{
  "name": "redash-client",
  "version": "9.0.1-beta",
  "description": "The frontend part of Redash.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:viz webpack-dev-server",
    "bundle": "bin/bundle-extensions",
    "clean": "rm -rf ./dist/",
    "clean-install:viz": "cd ../viz-lib && npm ci",
    "clean-install:plywood-client": "cd ../plywood/server/client && npm i ",
    "clean-install:plywood-server": "cd ../plywood/server && npm i ",
    "build:start": "npm run clean && npm install && npm run build && npm run start",
    "build:viz": "(cd ../viz-lib && export NODE_OPTIONS=--max_old_space_size=4096 && npm install && npm run build:babel)",
    "build:plywood-client": "(cd ../plywood/server/client && export NODE_OPTIONS=--max_old_space_size=4096 && npm run clear && npm install && npm run compile)",
    "build:plywood-server": "(cd ../plywood/server && export NODE_OPTIONS=--max_old_space_size=4096 && npm run clear && npm install && npm run build)",
    "build:plywood": "npm run build:plywood-client && npm run build:plywood-server",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run clean && npm install && npm run build:viz && npm run build:plywood && NODE_ENV=production webpack",
    "build:old-node-version": "npm run clean && NODE_ENV=production node --max-old-space-size=4096 node_modules/.bin/webpack",
    "watch:app": "webpack --watch --progress --colors -d",
    "watch:plywood": "(cd ../plywood/server/client && npm run watch)",
    "watch:viz": "(cd ../viz-lib && npm run watch:babel)",
    "watch": "npm-run-all --parallel watch:*",
    "webpack-dev-server": "webpack-dev-server",
    "analyze": "npm run clean && BUNDLE_ANALYZER=on webpack",
    "analyze:build": "npm run clean && NODE_ENV=production BUNDLE_ANALYZER=on webpack",
    "lint": "npm run lint:base -- --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint:fix": "npm run lint:base  -- --fix --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint:base": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore",
    "lint:ci": "npm run lint -- --max-warnings 0 --format junit --output-file /tmp/test-results/eslint/results.xml",
    "prettier": "prettier --write 'app/**/*.{js,jsx,ts,tsx}' 'cypress/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:watch": "npm run type-check -- --watch",
    "jest": "TZ=Africa/Khartoum jest",
    "test": "run-s type-check jest",
    "test:watch": "jest --watch",
    "cypress:install": "npm install --no-save cypress@~4.5.0 @percy/agent@0.26.2 @percy/cypress@^2.2.0 atob@2.1.2 lodash@^4.17.10 request-cookies@^1.1.0",
    "cypress": "node cypress/cypress.js",
    "preinstall": "npm run clean-install:plywood-server && npm run clean-install:plywood-client",
    "postinstall": "npm run clean-install:viz && npm run build:viz "
  },
