{
  "name": "reporter-plywood",
  "version": "0.24.0",
  "description": "A query planner and executor",
  "keywords": [
    "split",
    "apply",
    "combine",
    "druid",
    "query planner",
    "data reporter"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/dataminelab/datareporter.git"
  },
  "main": "build/plywood.js",
  "types": "build/index.d.ts",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "package/",
    "typings/"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf"
  },
  "watch": {
    "compile": {
      "patterns": [
        "src/**/*js",
        "src/**/*ts",
        "typings",
        "*.json"
      ]
    }
  },
  "scripts": {
    "watch": "nodemon",
    "prepare": "tdi -q",
    "build": "./compile",
    "compile": "./compile",
    "pretest": "./compile",
    "test": "./travis-test",
    "full-test": "./run-tests",
    "clear": "rm -rf build node_modules"
  },
  "nodemonConfig": {
    "ext": "*.ts,*.json,*.js",
    "exec": "npm run compile",
    "ignore": [
      "build/"
    ]
  },
  "tonicExampleFilename": "docs/examples/tonic.js",
  "dependencies": {
    "@aws-sdk/client-athena": "^3.3.0",
    "@druid-toolkit/query": "^0.19.1",
    "@google-cloud/bigquery": "^7.9.3",
    "chronoshift": "^0.10.0",
    "druid.d.ts": "^0.12.1",
    "has-own-prop": "^1.0.1",
    "immutable-class": "^0.9.4",
    "moment-timezone": "^0.5.26",
    "plywood-base-api": "^0.2.8",
    "readable-stream": "^2.3.5",
    "stream-to-array": "^2.3.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@types/stream-to-array": "^2.3.0",
    "awesome-code-style": "^1.4.3",
    "chai": "4.2.0",
    "f-tool": "0.1.3",
    "immutable-class-tester": "0.7.4",
    "imonit": "0.2.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "nodemon": "^2.0.4",
    "pegjs": "0.10.0",
    "plywood-druid-requester": "2.6.2",
    "plywood-mysql-requester": "2.3.0",
    "plywood-postgres-requester": "1.4.0",
    "tdi": "^0.6.5",
    "tslint": "5.18.0",
    "typescript": "^4.0.3"
  }
}
