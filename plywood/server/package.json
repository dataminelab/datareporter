{
  "name": "plywood-server",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --inspect=0.0.0.0:9229 -r ts-node/register dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "build": "tsc -p .",
    "full-build": "npm install && tsc -p .",
    "build:client": "cd client && npm install && npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch \"src/***\" --ext \"ts,json\" --exec \"ts-node src/server.ts\"",
    "ts-start": "ts-node src/server.ts",
    "clear": "rm -rf ./dist/",
    "full-clear": "rm -rf ./dist/ && rm -rf ./node_modules/"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^15.12.2",
    "body-parser": "^1.20.1",
    "chronoshift": "0.9.4",
    "druid-query-toolkit": "^0.9.16",
    "druid.d.ts": "^0.12.1",
    "has-own-prop": "^1.0.1",
    "immutable-class": "^0.9.4",
    "moment-timezone": "^0.5.26",
    "plywood-base-api": "^0.2.8",
    "readable-stream": "^3.0.3",
    "stream-to-array": "^2.3.0",
    "tslib": "^1.10.0",
    "concurrently": "^5.3.0",
    "d3": "^6.7.0",
    "express": "^4.18.2",
    "http-status": "^1.4.2",
    "immer": "^9.0.3",
    "immutable": "^4.0.0-rc.14",
    "immutable-class-tester": "^0.7.2",
    "l": "^0.6.0",
    "lz-string": "^1.4.4",
    "numbro": "^2.3.2",
    "plywood-postgres-requester": "^1.4.0",
    "react": "^17.0.2",
    "reporter-plywood": "file:client",
    "sinon": "^11.1.1",
    "stream-to-string": "^1.2.0",
    "tslog": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "@types/chai": "^4.2.12",
    "@types/chai-http": "^4.2.0",
    "@types/lz-string": "^1.3.34",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
