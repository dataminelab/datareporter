{
  "name": "plywood-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --inspect=0.0.0.0:9229 -r ts-node/register dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^15.12.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "d3": "^6.7.0",
    "@dataminelab/datareporter-plywood": "0.23.7",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "immer": "^9.0.3",
    "immutable": "^4.0.0-rc.14",
    "immutable-class-tester": "^0.7.2",
    "l": "^0.6.0",
    "lz-string": "^1.4.4",
    "nodemon": "^2.0.4",
    "numbro": "^2.3.2",
    "plywood-postgres-requester": "^1.4.0",
    "react": "^17.0.2",
    "sinon": "^11.1.1",
    "stream-to-string": "^1.2.0",
    "tslog": "^3.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/chai-http": "^4.2.0",
    "@types/lz-string": "^1.3.34",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
