{
  "name": "plywood-server",
  "license": "MIT",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --inspect=0.0.0.0:9229 -r ts-node/register dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/chai-http": "4.2.0",
    "@types/express": "4.17.13",
    "@types/lz-string": "1.3.34",
    "@types/mocha": "8.2.3",
    "chai": "4.3.4",
    "chronoshift": "^0.9.4",
    "concurrently": "5.3.0",
    "core-util-is": "1.0.2",
    "d3": "6.7.0",
    "datareporter-plywood": "file:../client",
    "escalade": "3.1.1",
    "express": "4.17.1",
    "http-status": "1.5.0",
    "immer": "9.0.5",
    "immutable": "4.0.0-rc.14",
    "immutable-class": "^0.9.4",
    "immutable-class-tester": "0.7.2",
    "isarray": "0.0.1",
    "l": "0.6.0",
    "lz-string": "1.4.4",
    "mocha": "8.4.0",
    "nodemon": "2.0.12",
    "numbro": "2.3.5",
    "p-try": "2.2.0",
    "plywood-postgres-requester": "1.4.0",
    "process-nextick-args": "2.0.1",
    "react": "17.0.2",
    "sinon": "11.1.2",
    "stream-to-string": "1.2.0",
    "ts-node": "9.1.1",
    "tslog": "3.2.0",
    "typescript": "4.3.5"
  }
}
